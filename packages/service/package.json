{
  "name": "@winterfox/service",
  "version": "0.1.0",
  "author": "SuYufox",
  "description": "SuYufox's Toolbox Service",
  "type": "module",
  "types": "./dist-js/index.d.ts",
  "main": "./dist-js/index.cjs",
  "module": "./dist-js/index.js",
  "exports": {
    ".": {
      "types": "./dist-js/index.d.ts",
      "import": "./dist-js/index.js",
      "require": "./dist-js/index.cjs"
    },
    "./log": {
      "import": "./dist/log/index.js",
      "require": "./dist/log/index.cjs",
      "types": "./dist/log/index.d.ts"
    }
  },
  "files": [
    "dist-js",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "pnpm build",
    "pretest": "pnpm build",
    "postinstall": "rollup -c"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0",
    "@winterfox/configs": "workspace:*",
    "@winterfox/plugins": "workspace:*"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.0.0",
    "rollup": "^4.9.6",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}